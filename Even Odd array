#include<iostream>
#include<vector>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    vector<int> e;
    vector<int> o;
    for(int i=0;i<n;i++)
    {
        if(arr[i]%2==0)
        {
            e.push_back(arr[i]);
        }
        else
        {
            o.push_back(arr[i]);
        }
    }
    for(int i=0;i<e.size();i++)
    {
        cout<<e[i]<<" ";
    }
    for(int i=0;i<o.size();i++)
    {
        cout<<o[i]<<" ";
    }
}
