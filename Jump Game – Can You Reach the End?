#include<iostream>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    for(int i=0;i<n;i++)
    {
        if(arr[i]==0)
        {
            cout<<"NO , You get stuck at the index "<<i;
            return 0;
        }
    }
    cout<<"YES , Start at index 0 -> jumped to index 1 -> extra -> jumped to last index "<<n;
}
