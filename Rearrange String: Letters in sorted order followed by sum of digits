#include<iostream>
#include<string>
using namespace std;
int main()
{
    string str;
    cin>>str;
    int len=str.length();
    char temp;
    for(int i=0;i<len;i++)
    {
        for(int j=0;j<len;j++)
        {
            if(str[i]<str[j])
            {
                temp=str[i];
                str[i]=str[j];
                str[j]=temp;
            }
        }
    }
    for(int i=0;i<len;i++)
    {
        if((str[i]>='A' && str[i]<='Z') || (str[i]>='a' && str[i]<='z'))
        {
            cout<<str[i];
        }
    }
    int sum=0;
    for(int i=0;i<len;i++)
    {
        if(str[i]>='0' && str[i]<='9')
        {
            sum=sum+(str[i]-'0');
        }
    }
    cout<<sum;
}
